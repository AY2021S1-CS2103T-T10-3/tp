<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.ListView?>
<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>

<StackPane alignment="CENTER_LEFT" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
    <stylesheets>
        <URL value="@Style.css" />
    </stylesheets>
    <children>
        <VBox alignment="TOP_CENTER" fx:id="statsBox">
            <children>
                <TextArea fx:id="pins" maxHeight="30.0" maxWidth="120.0" text="\$pins" editable="false" style="-fx-font-weight: bold"/>
                <ListView fx:id="listView" maxHeight="262.0" maxWidth="245.0" prefHeight="320.0" prefWidth="290.0" />
            </children>
        </VBox>
    </children>
</StackPane>
